{
  "name": "cds-tokens",
  "version": "0.9.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npm run transform && npm run resolve-tokens",
    "test": "echo \"Error: no test specified\" && exit 1",
    "transform": "npm run transform-light && npm run transform-dark",
    "transform-light": "npx token-transformer themes tokens/_cds-light/tokens.json core/cds-core,cds-light/theme-cds-light,components/type,components/form,components/spacing,components/navigation core/cds-core",
    "transform-dark": "npx token-transformer themes tokens/_cds-dark/tokens.json core/cds-core,cds-dark/theme-cds-dark,components/type,components/form,components/spacing,components/navigation core/cds-core",
    "resolve-tokens": "node ./build.js",
    "build-sass": "sass --style=compressed dist/scss/cds.scss dist/css/cds.min.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/compassion-global-experience/cds-tokens.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/compassion-global-experience/cds-tokens/issues"
  },
  "homepage": "https://github.com/compassion-global-experience/cds-tokens#readme",
  "dependencies": {
    "sass": "^1.55.0",
    "style-dictionary": "^3.7.1",
    "token-transformer": "^0.0.27"
  }
}
